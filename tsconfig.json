{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 20",
    "_version": "20.9.0",
    "compilerOptions": {
        "lib": [
            "es2023",
            "DOM"
        ],
        "outDir": "dist",
        "module": "NodeNext",
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noUncheckedIndexedAccess": true,
        "moduleResolution": "NodeNext",
    }
}